#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/language-server/bin/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/language-server/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/language-server/bin/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/language-server/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules/@angular/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/@angular+language-server@12.2.0/node_modules:/home/eko/.config/nvim/lua/custom/configs/languageserver/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ngserver" "$@"
else
  exec node  "$basedir/../../bin/ngserver" "$@"
fi
